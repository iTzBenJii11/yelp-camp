<% if (success.length > 0 ) { %>
<div class="alert alert-warning alert-dismissible fade show" role="alert">
  <strong><%= success %></strong>
  <!-- Display the 'message' variable instead -->
  <button
    type="button"
    class="btn-close"
    data-bs-dismiss="alert"
    aria-label="Close"
  ></button>
</div>
<% } %>
